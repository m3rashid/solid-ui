{
  "name": "solid-ui",
  "version": "0.0.0",
	"private": true,
	"license": "MIT",
  "workspaces": [
    "core",
    "docs",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest",
		"typescript": "5.0.4"
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=16.0.0"
  }
}
